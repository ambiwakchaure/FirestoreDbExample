<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.firestoredbexample">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".SubCollectionsActivity">
        </activity>
        <activity android:name=".TransactionActivity">
        </activity>
        <activity android:name=".BachWriteActivity">
        </activity>
        <activity android:name=".PaginationActivity">
        </activity>
        <activity android:name=".MultipleDocumentActivity">
        </activity>
        <activity android:name=".CrudActivity">
        </activity>
    </application>

</manifest>